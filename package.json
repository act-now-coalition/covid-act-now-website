    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "src/**/*.{css,scss,js}": [
      "eslint --fix src"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
